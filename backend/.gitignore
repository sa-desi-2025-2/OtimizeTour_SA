# ============================
# Laravel .gitignore completo
# ============================

# Dependências do Composer
/vendor/

# Arquivos de ambiente
.env
.env.backup
.env.testing
.env.local

# Cache e logs do framework
/storage/*.key
/storage/app/public
/storage/debugbar/
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*
/storage/logs/*

# Cache do bootstrap
/bootstrap/cache/*.php

# Arquivos gerados localmente
/node_modules/
npm-debug.log
yarn-error.log
yarn-debug.log
package-lock.json
pnpm-lock.yaml

# Compilação do Vite / Mix
/public/hot
/public/storage
/public/build
/public/mix-manifest.json

# Arquivos do sistema
.DS_Store
Thumbs.db
.idea/
.vscode/
*.log

# Testes e relatórios locais
coverage/
.phpunit.result.cache

# Backups ou arquivos pessoais
*.bak
*.tmp
*.swp
